{
  "name": "research-amp",
  "version": "1.0.0",
  "description": "The Research Area Mapping Platform",
  "author": "Social Science Research Council",
  "license": "GPL-3.0",
  "keywords": [
    "WordPress"
  ],
  "homepage": "https://ssrc.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hard-G/ramp.git"
  },
  "devDependencies": {
    "@wordpress/scripts": "^24.0.0",
    "fs-extra": "^11.1.1",
    "git": "^0.1.5",
    "rtlcss": "^3.5.0",
    "sass": "^1.42.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "build": "wp-scripts build assets/src/index.js assets/src/frontend.js assets/src/homepage-slides.js && npm run build-theme",
    "build-theme": "node scripts/build-theme.js",
    "check-engines": "wp-scripts check-engines",
    "watch": "node-sass sass/ -o ./ --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass sass/ -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'sass/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "start": "wp-scripts start assets/src/index.js assets/src/frontend.js assets/src/homepage-slides.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^5.0.3",
    "@dnd-kit/sortable": "^6.0.1",
    "@glidejs/glide": "^3.5.2",
    "@wordpress/api-fetch": "^5.2.4",
    "@wordpress/data": "^6.1.4",
    "@wordpress/url": "^3.2.3",
    "classnames": "^2.3.1",
    "dnd-kit": "^0.0.0",
    "react-dnd": "^15.1.1",
    "react-select": "^5.2.2"
  }
}
